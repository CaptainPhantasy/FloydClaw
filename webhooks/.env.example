# OpenClaw Webhook Server Configuration
# Copy this file to .env and fill in your values

# Server Configuration
WEBHOOK_PORT=3000
WEBHOOK_HOST=0.0.0.0

# OpenClaw Gateway
OPENCLAW_GATEWAY=ws://127.0.0.1:18789

# GitHub Webhook Secret
# Generate this in your GitHub repository settings
GITHUB_WEBHOOK_SECRET=your-github-webhook-secret-here

# Optional: External URL for webhook endpoint
# WEBHOOK_EXTERNAL_URL=https://your-domain.com

# Optional: Log level (debug, info, warn, error)
LOG_LEVEL=info
